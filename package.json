{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "db:reset": "tsx ./scripts/db-reset.ts",
    "db:seed": "tsx ./scripts/db-seed.ts",
    "db:studio": "drizzle-kit studio --config ./server/database/drizzle.config.ts",
    "dev": "nuxt dev",
    "format": "prettier  --list-different --write .",
    "generate": "nuxt generate",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "test": "exit 0",
    "typecheck": "nuxi typecheck",
    "validate": "npm run format && npm run lint:fix && npm run typecheck && npm test"
  },
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@lttr/puleo": "^0.5.0",
    "@zod/core": "^0.9.0",
    "@zod/mini": "4.0.0-beta.0",
    "compressorjs": "^1.2.1",
    "date-fns": "^4.1.0",
    "db0": "^0.3.2",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "exifreader": "^4.30.0",
    "heic2any": "^0.0.4",
    "nuxt": "^3.16.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@iconify-json/uil": "^1.2.3",
    "@lttr/nuxt-config-eslint": "^0.3.0",
    "@lttr/nuxt-config-postcss": "^0.0.8",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/plausible": "^1.2.0",
    "@nuxtjs/seo": "^3.0.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unplugin-vue-router": "^0.12.0",
    "vue-tsc": "^2.2.10"
  }
}
